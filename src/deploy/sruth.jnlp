<?xml version="1.0" encoding="UTF-8"?>
<jnlp spec="6.0+" 
        codebase="https://www.unidata.ucar.edu/staff/steve/sruth"
        <!-- codebase="file:///web/content/staff/steve/sruth" -->
        href="sruth.jnlp">
    <information>
        <title>Unidata Data Distribution Application</title>
        <vendor>University Corporation for Atmospheric Research</vendor>
        <homepage href="http://www.unidata.ucar.edu/staff/steve/sruth"/>
        <description kind="one-line">
            Unidata application for subscribing to Internet data publications.
        </description>
        <description kind="tooltip">
            Unidata data subscription application
        </description>
        <shortcut online="true">
            <desktop/>
            <menu submenu="Internet"/>
        </shortcut>
        <association extensions="sruth" mime-type="application/x-sruth">
            <description>Unidata data subscription</description>
        </association>
    </information>
    <resources>
        <!-- <property name="jnlp.packEnabled" value="true"/> -->
        <java version="1.7+"/>
        <jar href="sruth.jar" main="true"/>
    </resources>
    <application-desc/>
    <security>
        <all-permissions/>
    </security>
    <update check="always" policy="prompt-update"/>
</jnlp>
